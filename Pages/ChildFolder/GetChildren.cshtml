@page
@model MYPROJECT2.Pages.ChildFolder.GetChildrenModel
@{
}
<p>
    <a asp-page="/ChildFolder/AddChildren"> New Child</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th> ChildId </th>
            <th>Name </th>
            <th>Address </th>
             <th>Age </th>
            <th>GroupId</th>
            <th>Start</th>
        </tr>
    </thead>
    <tbody>

        @if (Model.children != null)
        {
            @foreach (var item in Model.children)
            {
                <tr>

                    <td> @item.ChildId </td>

                    <td> @item.Name </td>
                    <td> @item.Address</td>
                    <td> @item.Age</td>
                    <td> @item.GroupId</td>
                    <td> @item.Start</td>

                    <td>
                        <p>
                            <a asp-page="/ChildFolder/DeleteChild" asp-route-sid="@item.ChildId">Delete </a>
                            
                            <a asp-page="/ChildFolder/UpdateChild" asp-route-sid="@item.ChildId">Update </a>
                        </p>
                    </td>
                    
                </tr>
            }
        }
</table>